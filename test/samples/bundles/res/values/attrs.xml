<?xml version="1.0" encoding="utf-8"?>
<resources>
  <attr name="themePadding" format="reference|dimension" />
  <attr name="themeColor" format="reference|color" />
  <attr name="themeUnused" format="reference" />
  <declare-styleable name="WickedCoolButton">
    <attr name="a_boolean" format="boolean" />
    <attr name="fancy_effects">
      <flag name="overdrive" value="0x01" />
      <flag name="distortion" value="0x02" />
      <flag name="delay" value="0x04" />
      <flag name="reverb" value="0x08" />
    </attr>
    <attr name="reverb_type" format="enum">
      <enum name="hall" value="0"/>
      <enum name="spring" value="1"/>
      <enum name="plate" value="2"/>
      <enum name="shimmer" value="3"/>
    </attr>
    <attr name="unused_boolean_attr" format="boolean" />
    <attr name="dangerous" format="string" />
  </declare-styleable>

  <attr name="SameAttributeA">
    <flag name="overdrive" value="0x01" />
    <flag name="distortion" value="0x02" />
    <flag name="delay" value="0x04" />
    <flag name="reverb" value="0x08" />
  </attr>
  <attr name="SameAttributeB">
    <flag name="reverb" value="0x08" />
    <flag name="overdrive" value="0x01" />
    <flag name="delay" value="0x04" />
    <flag name="distortion" value="0x02" />
  </attr>

  <declare-styleable name="SameStyleableA">
    <attr name="a_boolean" format="boolean" />
    <attr name="b_boolean" format="boolean" />
  </declare-styleable>

  <declare-styleable name="SameStyleableB">
    <attr name="b_boolean" format="boolean" />
    <attr name="a_boolean" format="boolean" />
  </declare-styleable>
</resources>
